@import '../px-to-vw.styl'

.outer-wrapper {
  padding: 0;
  margin: 0;

  width: 100%;
  height: 100%;

  display: flex;

  .left-side {
    flex: 1 1 auto;
    width: $(580px);

    background: #e1e1e1;
  }

  .right-side {
    flex: 2 1 auto;
    width: $(700px);

    background: white;
  }
}
